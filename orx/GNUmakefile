	if test ! -d $(prefix)/include/orx/`dirname $$f` ; then \
	$(INSTALL) -d $(prefix)/include/orx/`dirname $$f`; \
	fi; \
	$(INSTALL) -m 644 include//$$f $(prefix)/include/orx/$$f; \
	done

