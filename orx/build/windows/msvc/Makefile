	-if exist ..\..\..\bin\plugins\demo\Cave.dll del ..\..\..\bin\plugins\demo\Cave.dll
	-if exist ..\..\..\bin\plugins\demo\Cave.ilk del ..\..\..\bin\plugins\demo\Cave.ilk
